    "tslint": "^6.1.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "jasmine-ts --config=jasmine.json",
